<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>

<% include ../partials/nav.ejs %>

<div class="container">
<h2>Database Results</h2>

<table width="100%" border="1">
  <tr>
    <th>wifidata_id</th>
    <th>station_id</th>
    <th>station_name</th>
    <th>latitude</th>
    <th>longitude</th>
    <th>mac_address</th>
    <th>floor</th>
    <th>zone_id</th>
    <th>zone_name</th>
    <th>gender</th>
    <th>age</th>
    <th>phone_prefix</th>
    <th>social</th>
    <th>registration_date</th>
    <th>connection_time</th>
    <th>send_time</th>
    <th>store_time</th>
  </tr>
  <% rows.forEach(function(r) { %>
    <tr>
      <td align="center"><%= r.wifidata_id %></td>
      <td align="center"><%= r.station_id %></td>
      <td align="center"><%= r.station_name %></td>
      <td align="center"><%= r.latitude %></td>
      <td align="center"><%= r.longitude %></td>
      <td align="center"><%= r.mac_address %></td>
      <td align="center"><%= r.floor %></td>
      <td align="center"><%= r.zone_id %></td>
      <td align="center"><%= r.zone_name %></td>
      <td align="center"><%= r.gender %></td>
      <td align="center"><%= r.age %></td>
      <td align="center"><%= r.phone_prefix %></td>
      <td align="center"><%= r.social %></td>
      <td align="center"><%= r.registration_date %></td>
      <td align="center"><%= r.connection_time %></td>
      <td align="center"><%= r.send_time %></td>
      <td align="center"><%= r.store_time %></td>
    </tr>
    <% }); %>
</table>

</div>

</body>
</html>
